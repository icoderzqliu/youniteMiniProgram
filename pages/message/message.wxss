.mainContent {
  margin: 15px 30px;
}

.resume-publish {
  border-radius: 15px;
  background-color: white;
}

.resume-publish>.resume-publish-title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
  margin-bottom: 5px;
}

.column {
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}

.row {
  display: flex;
  flex-direction: row;
}


.homeMainTeamCardActionBar {
  padding: 5px 5px 5px 5px;
  display: flex;
  flex-direction: row;
  min-height: 5px;
  align-items: flex-end;
}

.homeMainTeamCardActionBarBadge {
  display: flex;
  flex-direction: column;
}

.homeMainTeamCardActionBarBadge>.miniBadge {
  margin-bottom: 5px;
  min-width: 50rpx;
  color: white;
  text-align: center;
  border-radius: 9rpx;
  padding: 5rpx 15rpx;
}

.purple {
  background-color: rgb(109, 106, 239);
  color: var(--white);
}